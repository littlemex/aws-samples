# FIXME: TARGET_PLATFORM を追加してください

# AWS Account and Region
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1

# ECR Repository
ECR_REPO=${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_REGION}.amazonaws.com/rembg-async-app

# SageMaker Configuration
SAGEMAKER_ROLE_ARN=arn:aws:iam::${AWS_ACCOUNT_ID}:role/service-role/AmazonSageMaker-ExecutionRole
SAGEMAKER_MODEL_NAME=rembg-async-app
SAGEMAKER_ENDPOINT_NAME=rembg-async-app
SAGEMAKER_INSTANCE_TYPE=ml.g4dn.xlarge

# S3 Buckets
INPUT_BUCKET=your-input-bucket-name
OUTPUT_BUCKET=your-output-bucket-name

# SNS Topics
SUCCESS_TOPIC_ARN=arn:aws:sns:${AWS_REGION}:${AWS_ACCOUNT_ID}:RembgAsyncInferenceStack-AsyncInferenceTopic
ERROR_TOPIC_ARN=arn:aws:sns:${AWS_REGION}:${AWS_ACCOUNT_ID}:RembgAsyncInferenceStack-AsyncInferenceTopic

# Runtime Configuration
USE_GPU=true
MAX_CONCURRENT_INVOCATIONS=4
MODEL_NAME=u2net
MODEL_PATH=/opt/ml/model
