# MLflow Settings
MLFLOW_TRACKING_URI="http://localhost:5000"
MLFLOW_EXPERIMENT_NAME="litellm-monitoring"
DEPLOYMENT_ENV="local"  # local または aws

# MLflow Database Settings
POSTGRES_DB="mlflow"
